<!DOCTYPE html>
<html>
  <head>
    <title>Uncappd</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>
    <link rel="icon" href="https://uncapped-seed.s3-us-west-1.amazonaws.com/nav-logo.png" type="image/x-icon" >
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.9.0/css/all.min.css">
    <%= javascript_include_tag 'application' %>

    <%# <script type="text/javascript"> 
    window.system = window.navigator.platform
    </script> %>


    <script type="text/javascript">
      window.images = {
        grayStar: "<%= image_url('gray_star.jpg') %>",
        star: "<%= image_url('star.jpg') %>",
        main: "<%= image_url('beer.jpg') %>",
        genericBeer: "<%= image_url('generic_beer.jpg') %>",
        phones: "<%= image_url('phones.png') %>",
        navLogo: "<%= image_url('navlogo.png') %>",
        genericReview: "<%= image_url('generic_review.jpg') %>"
      }
    </script>


     <% if logged_in? %>
      <script type="text/javascript">
      window.currentUser = <%= render("api/users/user.json.jbuilder", user: current_user
      ).html_safe %></script>
      <% end %> 

   <head>

  <body>
    <%= yield %>
  </body>
</html>
